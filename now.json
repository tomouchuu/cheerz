{
  "version": 2,
  "alias": "cheerz.uchuu.io",
  "builds": [
    { "src": "./build/**/*", "use": "@now/static" },
    { "src": "./styleguide/**/*", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/docs/(.*)", "dest": "/styleguide/$1" },
    { "src": "/(.*)", "dest": "/build/$1" }
  ],
  "regions": ["bru"]
}